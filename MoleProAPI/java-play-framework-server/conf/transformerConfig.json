        "chebi": {
            "prefix": "CHEBI:",
            "uri": "https://www.ebi.ac.uk/chebi/searchId.do?chebiId=%s",
            "producer":"ChEBI compound-list producer"
        }
    },
    "curie prefix map":{
    	"PUBCHEM:":"CID:"
    },
    "expirationTimes": {
        "collections": "2w",
