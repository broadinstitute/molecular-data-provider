        "source_url": "https://www.nlm.nih.gov/research/umls/rxnorm/index.html",
        "source_version": "20AA_220307F (2022-03-07)",
        "terms_of_service": "https://www.nlm.nih.gov/research/umls/rxnorm/docs/termsofservice.html"
