{
    "name": "CTD pathway associations transformer",
    "label": "CTD",
    "description": "Transformer based on Comparative Toxicogenomics Database (http://ctdbase.org/).",
    "version": "2.3.0",
    "function": "transformer",
    "knowledge_map": {
        "input_class": "compound",
        "output_class": "pathway",
        "nodes": {
            "SmallMolecule": {
                "id_prefixes": ["CID","MeSH"]
            },
            "Pathway": {
                "id_prefixes": ["REACT", "KEGG"]
            }
        },
        "edges": [{
            "subject": "SmallMolecule",
            "predicate": "related_to",
            "inverse_predicate": "related_to",
            "object": "Pathway"
        }]
    },
    "properties": {
        "source_url": "http://ctdbase.org/",
        "source_version": "March 2021",
        "terms_of_service": "http://ctdbase.org/about/legal.jsp"
    },
    "parameters": [
        {
            "default":"0",
            "required":false,
            "name":"limit",
            "type":"string",
            "suggested_values":"maximum number of pathways to display"
        }

    ]
}
